{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1664224764.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"source_locale",
		"target_locale",
		"source_string",
		"human_needed"
	],
	"properties": {
		"source_locale": {
			"$id": "#root/source_locale", 
			"title": "Source locale", 
			"type": "string",
			"default": "",
			"examples": [
				"en"
			],
			"pattern": "^.*$"
		},
		"target_locale": {
			"$id": "#root/target_locale", 
			"title": "Target_locale", 
			"type": "string",
			"default": "",
			"examples": [
				"fr"
			],
			"pattern": "^.*$"
		},
		"source_string": {
			"$id": "#root/source_string", 
			"title": "Source_string", 
			"type": "string",
			"default": "",
			"examples": [
				"Hello"
			],
			"pattern": "^.*$"
		},
		"human_needed": {
			"$id": "#root/human_needed", 
			"title": "Use human (Gengo) or Machine (Google)", 
			"type": "boolean",
			"examples": [
				false, true
			],
			"default": true
		},
		"custom": {
			"$id": "#root/custom", 
			"title": "Custom attributes, passed through back with the result", 
			"type": "object"
		}

	}
}
