{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1664224764.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"source_locale",
		"target_locale",
		"source_string",
		"human_needed",
		"custom"
	],
	"properties": {
		"source_locale": {
			"$id": "#root/source_locale", 
			"title": "Source_locale", 
			"type": "string",
			"default": "",
			"examples": [
				"en"
			],
			"pattern": "^.*$"
		},
		"target_locale": {
			"$id": "#root/target_locale", 
			"title": "Target_locale", 
			"type": "string",
			"default": "",
			"examples": [
				"fr"
			],
			"pattern": "^.*$"
		},
		"source_string": {
			"$id": "#root/source_string", 
			"title": "Source_string", 
			"type": "string",
			"default": "",
			"examples": [
				"Hello"
			],
			"pattern": "^.*$"
		},
		"human_needed": {
			"$id": "#root/human_needed", 
			"title": "Human_needed", 
			"type": "boolean",
			"examples": [
				false
			],
			"default": true
		},
		"custom": {
			"$id": "#root/custom", 
			"title": "Custom", 
			"type": "object",
			"required": [
				"object",
				"page_id",
				"field_id"
			],
			"properties": {
				"object": {
					"$id": "#root/custom/object", 
					"title": "Object", 
					"type": "string",
					"default": "",
					"examples": [
						"page"
					],
					"pattern": "^.*$"
				},
				"page_id": {
					"$id": "#root/custom/page_id", 
					"title": "Page_id", 
					"type": "integer",
					"examples": [
						12345
					],
					"default": 0
				},
				"field_id": {
					"$id": "#root/custom/field_id", 
					"title": "Field_id", 
					"type": "string",
					"default": "",
					"examples": [
						"foo.56789"
					],
					"pattern": "^.*$"
				}
			}
		}

	}
}
